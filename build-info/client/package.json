{
  "name": "buildinfo",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "serve-dist": "ws --hostname localhost -d dist/buildinfo -p 1234 -o --log.format stats",
    "lint": "ng lint",
    "prebuild-prod": "node ./build-info-env.js",
    "build-prod": "ng build --prod",
    "postbuild-prod": "node ./git-tag.js",
    "release-patch": "npm --no-git-tag-version version patch && npm run build-prod"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "9.1.3",
    "@angular/compiler": "9.1.3",
    "@angular/core": "9.1.3",
    "@angular/platform-browser": "9.1.3",
    "@angular/platform-browser-dynamic": "9.1.3",
    "@angular/router": "9.1.3",
    "rxjs": "6.5.5",
    "tslib": "1.11.1",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.3",
    "@angular/cli": "9.1.3",
    "@angular/compiler-cli": "9.1.3",
    "@angular/language-service": "9.1.3",
    "@types/node": "13.13.2",
    "codelyzer": "5.2.2",
    "local-web-server": "4.0.0",
    "replace-in-file": "5.0.2",
    "simple-git": "1.132.0",
    "tslint": "6.1.1",
    "typescript": "3.8.3"
  }
}
